{
  "name": "svg-tree-vue3",
  "private": false,
  "description": "A Vue3 component for rendering tree in SVG",
  "version": "0.0.0",
  "type": "module",
  "types": "./dist/TreeNode.d.ts",
  "module": "./dist/TreeNode.js",
  "main": "./dist/TreeNode.cjs",
  "files": [
    "./dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/TreeNode.d.ts",
      "import": "./dist/TreeNode.js",
      "require": "./dist/TreeNode.cjs"
    },
    "./styles.css": "./dist/TreeNode.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc -b",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.10"
  }
}
