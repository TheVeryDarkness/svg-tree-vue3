{
  "name": "svg-tree-vue3",
  "private": false,
  "description": "A Vue3 component for rendering tree in SVG.",
  "keywords": [
    "vue3",
    "svg",
    "tree"
  ],
  "version": "0.5.0",
  "type": "module",
  "types": "./dist/Tree.d.ts",
  "module": "./dist/Tree.js",
  "main": "./dist/Tree.cjs",
  "files": [
    "./dist/**"
  ],
  "exports": {
    "./TreeNode.vue": {
      "types": "./dist/TreeNode.vue.d.ts",
      "import": "./dist/TreeNode.js",
      "require": "./dist/TreeNode.cjs"
    },
    ".": {
      "types": "./dist/Tree.vue.d.ts",
      "import": "./dist/Tree.js",
      "require": "./dist/Tree.cjs"
    },
    "./Tree.vue": {
      "types": "./dist/Tree.vue.d.ts",
      "import": "./dist/Tree.js",
      "require": "./dist/Tree.cjs"
    },
    "./Forest.vue": {
      "types": "./dist/Forest.vue.d.ts",
      "import": "./dist/Forest.js",
      "require": "./dist/Forest.cjs"
    },
    "./TreeV2.vue": {
      "types": "./dist/TreeV2.vue.d.ts",
      "import": "./dist/TreeV2.js",
      "require": "./dist/TreeV2.cjs"
    },
    "./ForestV2.vue": {
      "types": "./dist/ForestV2.vue.d.ts",
      "import": "./dist/ForestV2.js",
      "require": "./dist/ForestV2.cjs"
    },
    "./state": {
      "types": "./dist/state.d.ts",
      "import": "./dist/state.js",
      "require": "./dist/state.cjs"
    },
    "./svg": {
      "types": "./dist/svg.d.ts",
      "import": "./dist/svg.js",
      "require": "./dist/svg.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./auto.css": "./dist/auto.css",
    "./dark.css": "./dist/dark.css",
    "./light.css": "./dist/light.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "dev-demo": "vite dev -c vite.demo.config.ts",
    "preview-demo": "vite preview -c vite.demo.config.ts",
    "build-demo": "vite build -c vite.demo.config.ts",
    "format:check": "prettier --check .",
    "prepublishOnly": "vue-tsc -b && vite build"
  },
  "dependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "prettier": "^3.6.2",
    "typescript": "~5.6.3",
    "vite": "^7.0.1",
    "vite-plugin-dts": "^4.5.4",
    "vue-component-type-helpers": "^3.0.3",
    "vue-tsc": "^3.0.1"
  }
}
